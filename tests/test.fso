(include "stdio.h")
(include "stdlib.h")

(func printf [ (fmt (@ U8)) (abc CVarArgs) ] Void)

(func hello [(name (@ U8))] Void {
	(printf "Hello, %s!\n" name)
})

(func main [(argc I32) (argv (@ (@ U8)))] I32 {
	(decl test I32)
	(set test 2)
	(set test (+ test 2))

	(if [= argc 1] {
		(hello "Turing Tarpit")
		(printf "Value: %d\n" test)
	})

	(decl i I32)
	(set i 0)

	(while [< i 10] {
		(printf "I: %d\n" i)
		(set i (+ i 1))
	})

	(printf "%d %d %d" 1 2 3)

	(return 0)
})
